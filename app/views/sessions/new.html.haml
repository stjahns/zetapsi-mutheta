- provide(:title, "Log in")

%H1 Log in
= form_for(:session, url: sessions_path) do |f|

  .row
    .span
      = f.label :email
      = f.text_field :email

  .row
    .span
      = f.label :password
      = f.password_field :password

  .row
    .span
      = f.submit "Log in", class: "btn btn-primary"

